<swishconfig>
 <!-- v attr indicates what handler should do with property buffer -->
 <PropertyNames>title</PropertyNames>
 <PropertyNames type="nothing">fooname</PropertyNames>
 <PropertyNames type="numeric">someNum</PropertyNames>
 <PropertyNames type="comparecase">SoMeCaSe</PropertyNames>
 <PropertyNames type="ignorecase">SoMeOTHERCaSe</PropertyNames>
 <PropertyNames type="date">dAte</PropertyNames>
 <PropertyNames type="nostripchars">propWithNewlines</PropertyNames>
 <!-- these are used when buffering and storing properties -->
 <PropertyNamesMaxLength type="1000">fooname</PropertyNamesMaxLength>
 <PropertyNamesSortKeyLength type="100">fooname</PropertyNamesSortKeyLength>
</swishconfig>
